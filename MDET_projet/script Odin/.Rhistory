X = matrix(rep(x,i), ncol = i, nrow = i, byrow = TRUE)
#Calcul de la matrice M
D = t(X) - X
#Resolution du systeme d'ODE
model <- function(t,N,sigma){
M = exp(-0.5*D^2/(sigma^2))/t(K)
dN <- r * N * (1 - N %*% M)
return(list(dN))
}
